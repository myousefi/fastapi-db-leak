# Runtime environment
ENVIRONMENT=local
PROJECT_NAME="FastAPI DB Leak Demo"
BACKEND_CORS_ORIGINS="http://localhost:8000"
SECRET_KEY=0258e4f95fc1cfafa8787c44f9a6a5a7097080c34b51f1774ba5fd9b3212848f
FIRST_SUPERUSER=admin@example.com
FIRST_SUPERUSER_PASSWORD=super_user_pass

# Database settings
POSTGRES_SERVER=db
POSTGRES_PORT=5432
POSTGRES_DB=app
POSTGRES_USER=postgres
POSTGRES_PASSWORD=pgpass

# Connection pool configuration
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=10
DB_POOL_TIMEOUT=3.0

# Container image tag for the backend service
DOCKER_IMAGE_BACKEND=backend

# Observability (SigNoz / OpenTelemetry)
OTEL_TRACING_ENABLED=true
OTEL_LOGS_ENABLED=true
# Defaults to the SigNoz collector's gRPC endpoint when using the provided docker-compose setup.
OTEL_EXPORTER_OTLP_ENDPOINT=http://host.docker.internal:4317
OTEL_EXPORTER_OTLP_INSECURE=true
OTEL_EXPORTER_OTLP_TIMEOUT=10.0
OTEL_EXPORTER_OTLP_HEADERS=
OTEL_SERVICE_NAME="fastapi-experiments"
OTEL_SERVICE_VERSION=local
OTEL_FASTAPI_EXCLUDED_URLS=
OTEL_SQLCOMMENTER_ENABLED=true
OTEL_LOG_LEVEL=INFO
